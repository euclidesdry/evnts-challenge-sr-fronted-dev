{
	"name": "pokemon-app",
	"private": true,
	"version": "3.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint src --ext .js,ts,jsx,tsx,json",
		"lint:fix": "eslint src --ext .js,ts,jsx,tsx,json --fix",
		"lint:format": "prettier --write src/**/*.{js,ts,tsx,jsx,html,css,md,json}",
		"test": "jest --maxWorkers=4 --onlyChanged",
		"test:all": "jest --maxWorkers=4 --detectOpenHandles",
		"test:watch": "jest --watch --onlyChanged --detectOpenHandles",
		"test:coverage": "jest --coverage --maxWorkers=4",
		"test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
		"test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
		"pkg:check": "npx npm-check",
		"pkg:check:fix": "npx npm-check --install",
		"pkg:check:fix:deps": "npx npm-check --install --skip-unused",
		"pkg:check:fix:devDeps": "npx npm-check --install --skip-unused --dev",
		"pkg:check:fix:all": "npx npm-check --install --skip-unused --all",
		"pkg:check:update": "npx npm-check -u",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,tsx,jsx}": "lint:fix",
		"*.{js,ts,tsx,jsx,css,md,json}": "lint:format"
	},
	"dependencies": {
		"@tanstack/react-query": "^4.28.0",
		"@tanstack/react-query-devtools": "^4.29.0",
		"@types/styled-components": "^5.1.26",
		"axios": "^1.3.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"jest-transform-stub": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-paginate": "^8.1.5",
		"react-transition-group": "^4.4.5",
		"styled-components": "^5.3.9"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/plugin-syntax-jsx": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@testing-library/cypress": "^9.0.0",
		"@testing-library/dom": "^9.2.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.33",
		"@types/react-dom": "^18.0.11",
		"@types/react-transition-group": "^4.4.5",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitejs/plugin-react": "^3.1.0",
		"babel-jest": "^29.5.0",
		"babel-preset-react-app": "^10.0.1",
		"cypress": "^12.9.0",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-testing-library": "^5.10.2",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.5.0",
		"jest-circus": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-watch-typeahead": "^2.2.2",
		"lint-staged": "^13.2.0",
		"msw": "^1.2.1",
		"prettier": "^2.8.7",
		"sass": "^1.60.0",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"vite": "^4.2.0"
	}
}
